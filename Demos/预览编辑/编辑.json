type:
[
    0:单选,
    1:多选,
    2:填空
]


// 问卷 formData
{
    "id":0,// 问卷编号
    "title":"",// 问卷标题
    "questions":[
        {
            "show":false,// 在编辑时是表示编辑面板的显示，在数据库表示题目是否显示
            "id":0,// 题目 id（还是要有的，如果题目发生移动，光用下标来标记会很麻烦）
            "type":0,// 题目类型
            "title":"题目",// 题目

            /* 多选题选项数量限制 */

            "must":false,// 是否必答
            // 当选择id为question_id的题目中的选项id为option_id中任意一个时，本题会出现
            "rela":{
                "question_id":null,
                "option_index":[],
            }

            /* 选择题/填空题 */

        }
    ]
}


// 多选题选项数量限制
"min":2,// min >= 2
"max":null,// null表示不对最大数量进行限制


// 选择题
"options":[
    {
        "content":"选项一",
    },
    {
        "content":"选项二",
    },
    {
        "content":"选项二",
    },
]



// 填空题
{
   "content":"填空的内容"
}



