// 问卷 formData
{
    "userId":0,
    "id":0,// 问卷编号
    "title":"",// 问卷标题
    "questions":[
        {
            "show":false,// 在编辑时是表示编辑面板的显示，在数据库表示题目是否显示
            "id":0,// 题目 id（还是要有的，如果题目发生移动，光用下标来标记会很麻烦）
            "type":0,// 题目类型
            "err":false,// 如果题目填写不符合要求为true，在编辑时默认为false
            /* 多选题选项数量限制 */


            "title":"题目",// 题目
            "must":false,// 是否必答
            /* 选择题/填空题 */
        }
    ]
}




